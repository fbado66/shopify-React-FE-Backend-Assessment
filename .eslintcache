ectedFile: e.target.files[0]\n    })\n  }\n\n  fileUploadHandler = () => {\n    const fd = new FormData()\n    fd.append('file', this.state.selectedFile, this.state.selectedFile.name)\n    fd.append('upload_preset', 'snapupy')\n      fetch('https://api.cloudinary.com/v1_1/personal-space/image/upload', {\n        method: \"POST\",\n        body: fd\n      })\n    .then(res => res.json())\n    .then(image_url => {\n      this.setState({\n        image: image_url.secure_url\n      })\n    })\n  }\n\n  handleChange = (e) => {\n    let {name, value} = e.target\n    this.setState({\n      [name]: value\n    })\n  }\n\n  render() {\n\n    let {name, description, price, category} = this.state\n    return (\n      <div>\n        <Form onSubmit={this.handleSubmit}>\n          <Form.Field>\n            <label>Name</label>\n            <input placeholder='Name'\n                    autoComplete=\"off\"\n                    name ='name'\n                    value={name}\n                    onChange={this.handleChange} />\n          </Form.Field>\n          <Form.Field>\n            <label>Description</label>\n            <input placeholder='What is your product about?'\n                    autoComplete=\"off\"\n                    name ='description'\n                    value={description}\n                    onChange={this.handleChange} />\n          </Form.Field>\n          <Form.Field>\n            <label>Image</label>\n            <img id='imgProductOnForm' src={this.state.image ? this.state.image : './../img_placeholder.png'} />\n            <input type=\"file\"\n                    id='uploadButton'\n                    name=\"image\"\n                    onChange={e => {this.fileSelectedHandler(e); \n                      setTimeout( () =>this.fileUploadHandler(), 500)\n                    }} />\n          </Form.Field>\n          <Form.Field>\n            <label>Price</label>\n            <input placeholder='Price'\n                    type='number'\n                 